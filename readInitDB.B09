PROCEDURE readInitDB(* Version 1.3.00 *)(* File Record *)TYPE VFP1=vdTkn:BYTE; vdPtr,vdSiz,dsPtr,dsvSiz,dsSiz,dmSiz,fSiz,pSiz,elem1,elem2,elem3,vSRLen:INTEGERTYPE VFP2=vArray:STRING[19]; vName:STRING[29]; vStrLen:STRING[7]; vRecNum,vRefCnt,intReal,pVar:INTEGERTYPE VFP3=mTyp,vLnk,fNum:BYTE; pRec,pFld:BOOLEANTYPE VFRC= vp1:VFP1; vp2:VFP2; vp3:VFP3DIM varFRec:VFRC(* DSMap - DSAT Map *)TYPE DSMP=dsOf,dsDm,dsMSz,dsSz:INTEGERDIM DSMap(100):DSMP(* Colors *)REM TYPE COLR=white,purple,black,green,yellow,pink,cyan,blue,white2,purple2,black2,green2,yellow2,pink2,cyan2,blue2:BYTETYPE COLR=white,blue,black,green,red,yellow,magenta,cyan,white2,blue2,black2,green2,red2,yellow2,magenta2,cyan2:BYTEDIM color:COLRDIM oPath,token(256),options(26),defFcol,defBcol:BYTEDIM cntr,row:INTEGERDIM oOpen:BOOLEANDIM version:STRING[8]; dataDir,oFile:STRING[29]; screenLn(18):STRING[79]BASE 0dataDir:="/DD/DATA/"oFile:=dataDir+"initDB"PRINT "Reading initDB"; OPEN #oPath,oFile:READoOpen:=TRUEGET #oPath,versionGET #oPath,optionsGET #oPath,defFcolGET #oPath,defBcolGET #oPath,tokenGET #oPath,colorGET #oPath,varFRecGET #oPath,DSMapFOR cntr:=0 TO 17GET #oPath,screenLn(cntr)NEXT cntrCLOSE #oPathoOpen:=FALSEPRINT "Current version: "; versionPRINT "variable size: "; SIZE(version)PRINT "Options"PRINT "Display Options at Startup: "; options(0)(* Colors *)PRINT "Window Foreground Color: "; options(1),PRINT "Window Background Color: "; options(2)PRINT "Back Overlay Foreground Color: "; options(3),PRINT "Back Overlay Background Color: "; options(4)PRINT "Middle Overlay Foreground Color: "; options(5),PRINT "Middle Overlay Background Color: "; options(6)PRINT "Front Overlay Foreground Color: "; options(7),PRINT "Front Overlay Background Color: "; options(8)(* Window Views *)PRINT "View Processes (On/Off): "; options(9)(* decode windows *)PRINT "View Instruction Decode: "; options(10),PRINT "View Token Counts: "; options(11)PRINT "View Line Reference List (unnumbered): "; options(12),PRINT "View Line Reference List (numbered): "; options(13)(* defVars windows *)PRINT "View Variable Reference List (unsorted): "; options(14),PRINT "View Variable Reference List (naming): "; options(15)PRINT "View Variable Reference List (sorted): "; options(16),PRINT "View VDT Validation: "; options(17) (* buileSrc windows *)PRINT "View DSAT Count: "; options(18),PRINT "View DSAT Validation: "; options(19)PRINT "View Source Output: "; options(20)(* File  options*)PRINT "Create <modulename>Raw.txt file: "; options(21),PRINT "Create <modulename>Lines.txt file: "; options(22)PRINT "Create <modulename>Vars.txt file: "; options(23),PRINT "Create <modulename>VDT.txt file: "; options(24)PRINT "Create <modulename>DSAT.txt file: "; options(25)PRINT "Default Window Foreground Color: "; defFcol,PRINT "Default Window Background Color: "; defBcolrow:=0FOR cntr:= 0 TO 255  PRINT token(cntr); " ";  row:=row+1  IF row=16 THEN    row:=0    PRINT  ENDIFNEXT cntr