PROCEDURE ureadLins(* Line Reference Record *)(* Record *)TYPE LNREC=lnRecNum,lnRefCnt:INTEGER; lnTyp:BYTE; lnOff:INTEGER; lnNum:STRING[5]DIM linRec:LNRECDIM linCount,lnCnt:INTEGERDIM lnFile:STRING[29]DIM lnPath:BYTEDIM lnOpen,quit:BOOLEANBASE 0lnFile:="/DD/DATA/UNPACK/linDefs"REM dFile:="/DD/DATA/procData"lnOpen:=FALSEREM dOpen:=FALSEquit:=FALSElnCnt:=0OPEN #lnPath,lnFile:UPDATElnOpen:=TRUEGET #lnPath,linCountPRINT "Total Line Reference Records: "; linCountPRINT "Rec#  RfCt Tk Off Num"REPEAT SEEK #lnPath,lnCnt*SIZE(linRec)+2GET #lnPath,linRecPRINT USING "i5>,i5>,s1,h2,s1,h4,s30>",linRec.lnRecNum,linRec.lnRefCnt," ",linRec.lnTyp," ",linRec.lnOff,linRec.lnNumlnCnt:=lnCnt+1UNTIL lnCnt=linCountCLOSE #lnPathlnOpen:=FALSEEND 